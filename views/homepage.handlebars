<h1>Homepage</h1>
{{#each posts}}
  <div>
    <h2>{{this.title}}</h2>
    <p>{{this.content}}</p>
    <p>Posted by: {{this.user.username}}</p>
  </div>
{{/each}}

{{#if logged_in}}
  <a href="/dashboard">Go to Dashboard</a>
{{else}}
  <a href="/login">Login</a>
  |
  <a href="/signup">Sign Up</a>
{{/if}}